  w04 = 2 * $pi * freq4/srate;
  cosw04 = cos(w04);
  sinw04 = sin(w04);
  alpha4 = sinw04 / (2 * q4);

  b04 = 1 + alpha4 * a4;
  b14 = -2 * cosw04;
  b24 = 1 - alpha4 * a4;
  a04 = 1 + alpha4 / a4;
  a14 = -2 * cosw04;
  a24 = 1 - alpha4 / a4;
  b04 /= a04;
  b14 /= a04;
  b24 /= a04;
  a14 /= a04;
