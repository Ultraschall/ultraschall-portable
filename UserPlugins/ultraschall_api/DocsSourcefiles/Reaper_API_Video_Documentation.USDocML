                                   -     'B' - Bold
                                   -     'I' - Italics
                                   -     'R' - Blur
                                   -     'V' - Invert
                                   -     'M' - Mono
                                   -     'S' - Shadow
                                   -     'O' - Outline
                                   -    example: 'BI'
