    <US_DocBloc version="1.0" spok_lang="en" prog_lang="*">
        <slug>input_track_exact_count</slug>
        <title>input_track_exact_count</title>
        <functioncall prog_lang="eel">nteger num_tracks = input_track_exact_count()</functioncall>
        <requires>
            Reaper=6.13
