        <functioncall prog_lang="cpp">void ImGui_Attach(ImGui_Context* ctx, ImGui_Resource* obj)</functioncall>
        <functioncall prog_lang="eel">extension_api("ImGui_Attach", ImGui_Context ctx, ImGui_Resource obj)</functioncall>
